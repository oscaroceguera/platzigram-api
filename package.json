{
  "name": "platzigram-api",
  "version": "1.0.0",
  "description": "Platzigram Rest API",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && NODE_ENV=test ava --require babel-register test/**-test.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscaroceguera/platzigram-api.git"
  },
  "author": "Oscar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oscaroceguera/platzigram-api/issues"
  },
  "homepage": "https://github.com/oscaroceguera/platzigram-api#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^7.0.0",
    "babel-register": "^6.16.3",
    "standard": "^8.4.0",
    "test-listen": "^1.0.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "http-hash": "^2.0.0",
    "jsonwebtoken": "^7.1.9",
    "micro": "4.1.1",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "token-extractor": "^0.1.6",
    "uuid-base62": "^0.1.0"
  }
}
